<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="@drawable/appbar"
    android:layout_height="match_parent"
    tools:context=".JadwalFragment">
    <TextView
        android:id="@+id/jadwal"
        android:layout_marginTop="150dp"
        android:text="Jadwal Waktu Shalat"
        android:textSize="35dp"
        android:textColor="#757575"
        android:textAlignment="center"
        android:textStyle="bold"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <TextView
        android:id="@+id/hari"
        android:layout_below="@+id/jadwal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:text="value"
        android:textColor="#757575"
        android:textSize="25dp"
        android:textStyle="italic" />

    <RelativeLayout
        android:layout_below="@+id/hari"
        android:layout_marginTop="30dp"
        android:background="@drawable/background_jadwal"
        android:layout_centerInParent="true"
        android:layout_marginRight="60dp"
        android:layout_width="match_parent"
        android:layout_height="210dp">


        <TextView
            android:id="@+id/subuh"
            android:layout_marginTop="30dp"
            android:layout_marginLeft="2dp"
            android:background="@drawable/bg_jdwl"
            android:text="Subuh"
            android:textSize="20dp"
            android:textAlignment="center"
            android:layout_width="173dp"
            android:layout_height="wrap_content"/>
        <TextView
            android:id="@+id/subuh_value"
            android:layout_marginTop="30dp"
            android:background="@drawable/bg_jdwl"
            android:textSize="20dp"
            android:layout_toRightOf="@id/subuh"
            android:text="value"
            android:textAlignment="center"
            android:layout_width="174dp"
            android:layout_height="wrap_content"/>
        <TextView
            android:id="@+id/dhuhur"
            android:layout_marginTop="5dp"
            android:layout_marginLeft="2dp"
            android:layout_below="@+id/subuh"
            android:text="Dhuhur"
            android:textSize="20dp"
            android:background="@drawable/bg_jdwl"
            android:textAlignment="center"
            android:layout_width="173dp"
            android:layout_height="wrap_content"/>
        <TextView
            android:id="@+id/dhuhur_value"
            android:layout_marginTop="5dp"
            android:layout_below="@+id/subuh_value"
            android:background="@drawable/bg_jdwl"
            android:textSize="20dp"
            android:layout_toRightOf="@id/dhuhur"
            android:textAlignment="center"
            android:text="value"
            android:layout_width="174dp"
            android:layout_height="wrap_content"/>
        <TextView
            android:id="@+id/ashar"
            android:layout_marginLeft="2dp"
            android:layout_marginTop="5dp"
            android:layout_below="@+id/dhuhur"
            android:text="Ashar"
            android:textSize="20dp"
            android:background="@drawable/bg_jdwl"
            android:textAlignment="center"
            android:layout_width="173dp"
            android:layout_height="wrap_content"/>
        <TextView
            android:id="@+id/ashar_value"
            android:layout_below="@+id/dhuhur_value"
            android:text="value"
            android:layout_marginTop="5dp"
            android:textSize="20dp"
            android:background="@drawable/bg_jdwl"
            android:layout_toRightOf="@id/ashar"
            android:textAlignment="center"
            android:layout_width="174dp"
            android:layout_height="wrap_content"/>
        <TextView
            android:id="@+id/maghrib"
            android:layout_marginLeft="2dp"
            android:layout_marginTop="5dp"
            android:layout_below="@+id/ashar"
            android:text="Maghrib"
            android:textSize="20dp"
            android:background="@drawable/bg_jdwl"
            android:textAlignment="center"
            android:layout_width="173dp"
            android:layout_height="wrap_content"/>
        <TextView
            android:id="@+id/maghrib_value"
            android:layout_below="@+id/ashar_value"
            android:text="value"
            android:layout_marginTop="5dp"
            android:textSize="20dp"
            android:background="@drawable/bg_jdwl"
            android:layout_toRightOf="@id/maghrib"
            android:textAlignment="center"
            android:layout_width="174dp"
            android:layout_height="wrap_content"/>
        <TextView
            android:id="@+id/isha"
            android:layout_marginTop="5dp"
            android:layout_marginLeft="2dp"
            android:layout_below="@+id/maghrib"
            android:text="Isha"
            android:textSize="20dp"
            android:background="@drawable/bg_jdwl"
            android:textAlignment="center"
            android:layout_width="173dp"
            android:layout_height="wrap_content"/>
        <TextView
            android:id="@+id/isha_value"
            android:layout_below="@+id/maghrib_value"
            android:text="value"
            android:layout_marginTop="5dp"
            android:textSize="20dp"
            android:background="@drawable/bg_jdwl"
            android:layout_toRightOf="@id/isha"
            android:textAlignment="center"
            android:layout_marginBottom="5dp"
            android:layout_width="174dp"
            android:layout_height="wrap_content"/>

    </RelativeLayout>
</RelativeLayout>
